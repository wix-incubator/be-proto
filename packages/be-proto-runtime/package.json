{
  "name": "be-proto",
  "version": "1.0.0",
  "author": "Mantas Indra≈°ius <mantasi@wix.com>",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc --declaration && npm run build:deps && npm run copy:proto",
    "build:deps": "lerna bootstrap --no-ci && lerna run build",
    "copy:proto": "mkdir -p proto && rsync -aP private-deps/proto/* proto/",
    "test": "mocha http test test/http codegen server/test/**/*.e2e.js --reporter mocha-env-reporter",
    "release": "wnpm-release --no-shrinkwrap"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "lerna": "^3.18.1",
    "mocha": "^5.2.0",
    "mocha-env-reporter": "^3.0.0",
    "node-fetch": "^2.6.0",
    "wnpm-ci": "^6.2.55"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "long": "^4.0.0",
    "protobufjs": "^6.8.8",
    "typescript": "^3.6.4",
    "url-pattern": "^1.0.3"
  }
}
